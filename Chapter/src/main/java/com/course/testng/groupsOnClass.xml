        </groups>
        <classes>
            <class name="com.course.testng.groups.groupsOnClass"/>
            <class name="com.course.testng.groups.groupsOnClass2"/>
            <class name="com.course.testng.groups.groupsOnClass3"/>
        </classes>
    </test>
</suite>